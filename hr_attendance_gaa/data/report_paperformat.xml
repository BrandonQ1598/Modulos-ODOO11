<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="paperformat_carta" model="report.paperformat">
            <field name="name">Lista de asistencia</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">216</field>
            <field name="page_width">279</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">2</field>
            <field name="dpi">130</field>
        </record>

        <record id="paperformat_ticket_attendance" model="report.paperformat">
            <field name="name">Trabajo del d√≠a ticket</field>
            <field name="default" eval="True"/>
